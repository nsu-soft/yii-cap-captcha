{
    "name": "nsu-soft/yii-cap-captcha",
    "description": "Interface for accessing a stand-alone server with Cap Captcha",
    "keywords": [
        "yii2",
        "cap",
        "captcha",
        "security"
    ],
    "version": "0.1",
    "type": "library",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/nsu-soft/yii-cap-captcha/issues",
        "source": "https://github.com/nsu-soft/yii-cap-captcha"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "yiisoft/yii2": "~2.0.50",
        "psr/http-client": "^1.0"
    },
    "require-dev": {
        "codeception/codeception": "^5.3",
        "codeception/module-asserts": "^3.3",
        "codeception/module-yii2": "^2.0",
        "yiisoft/yii2-httpclient": "^2.0",
        "guzzlehttp/guzzle": "^7.10",
        "symfony/http-client": "^7.4",
        "nyholm/psr7": "^1.8"
    },
    "suggest": {
        "yiisoft/yii2-httpclient": "Allows you to access the Cap Captcha API in a native way",
        "psr/http-client-implementation": "Allows you to access the Cap Captcha API using third-party libraries that support the PSR standard"
    },
    "autoload": {
        "psr-4": {
            "nsusoft\\captcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
